<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <size
                android:width="16dp"
                android:height="16dp" />
        </shape>
    </item>

    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <shape android:shape="oval">
            <stroke
                android:width="0.5dp"
                android:color="#f5f5f5" />
            <size
                android:width="16dp"
                android:height="16dp" />
        </shape>
    </item>

</layer-list>